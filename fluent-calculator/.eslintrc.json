{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["google", "plugin:prettier/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "require-jsdoc": 0
    }
}
